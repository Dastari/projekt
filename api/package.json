{
  "private": true,
  "name": "api",
  "version": "1.0.1",
  "author": "Toby Martin <toby.martin@me.com>",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc --build",
    "start": "npm run build && NODE_ENV=production node build/index.js",
    "lint": "eslint src",
    "check": "npx npm-check-updates -x graphql",
    "update": "npx npm-check-updates -u -x graphql",
    "push": "npx prisma db push",
    "seed": "npx prisma db seed",
    "generate": "npx prisma generate",
    "dev": "npx nodemon ./src/index.ts",
    "studio": "npx prisma studio"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:@typescript-eslint/recommended"
    ],
    "ignorePatterns": [
      "build/"
    ]
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.0",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "bcrypt": "^5.1.0",
    "eslint": "^8.25.0",
    "graphql-scalars": "^1.19.0",
    "prisma": "^4.3.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typegraphql-prisma": "^0.21.5",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/module-alias": "^2.0.1",
    "apollo-server": "^3.10.3",
    "apollo-server-express": "^3.10.3",
    "express": "^4.18.2",
    "generate-passphrase": "^1.2.0",
    "graphql": "^15.8.0",
    "graphql-type-json": "^0.3.2",
    "graphql-ws": "^5.11.2",
    "jsonwebtoken": "^8.5.1",
    "ws": "^8.9.0"
  }
}
